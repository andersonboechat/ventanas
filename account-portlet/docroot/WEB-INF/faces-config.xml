<?xml version="1.0"?>

<faces-config
    version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
>
	<application>
		<message-bundle>Language</message-bundle>
	</application>
	<converter>
		<display-name>ParkingConverter</display-name>
		<converter-id>parkingConverter</converter-id>
		<converter-class>br.com.abware.accountmgm.util.faces.ParkingConverter</converter-class>
	</converter>
	<converter>
		<display-name>ModelListConverter</display-name>
		<converter-id>modelListConverter</converter-id>
		<converter-class>br.com.atilo.jcondo.commons.faces.ModelListConverter</converter-class>
	</converter>
	<converter>
		<display-name>SelectItemsConverter2</display-name>
		<converter-id>selectItemsConverter2</converter-id>
		<converter-class>br.com.atilo.jcondo.commons.faces.SelectItemsConverter</converter-class>
	</converter>
	<converter>
		<display-name>JsonConverter</display-name>
		<converter-id>jsonConverter</converter-id>
		<converter-class>br.com.atilo.jcondo.commons.faces.JsonConverter</converter-class>
	</converter>
	<lifecycle>
		<phase-listener>com.liferay.faces.util.lifecycle.DebugPhaseListener</phase-listener>
	</lifecycle>
</faces-config>